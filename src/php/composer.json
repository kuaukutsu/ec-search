{
  "name": "kuaukutsu/ec-search",
  "description": "Example Code: full text search (FTS)",
  "license": "MIT",
  "type": "lib",
  "authors": [
    {
      "name": "Dmitriy Krivopalov",
      "email": "dimarik82@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.4",
    "guzzlehttp/guzzle": "^7.1",
    "kuaukutsu/poc-migration": "^0.3",
    "manticoresoftware/manticoresearch-php": "^4.0",
    "opensearch-project/opensearch-php": "^2.5.1",
    "php-di/php-di": "^7.0",
    "symfony/console": "^7.4"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.1",
    "phpstan/phpstan": "^2.0",
    "rector/rector": "^2.0",
    "roave/security-advisories": "dev-latest",
    "slevomat/coding-standard": "^8.7",
    "squizlabs/php_codesniffer": "^3.7",
    "vimeo/psalm": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "kuaukutsu\\ec\\search\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "php-http/discovery": true
    },
    "platform": {
      "php": "8.4.8"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
